    "commitizen": "^4.3.1",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "cssnano": "^7.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.8",
